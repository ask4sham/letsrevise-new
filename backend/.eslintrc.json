{
  "env": {
    "node": true,
    "es2022": true,
    "browser": true
  },
  "extends": ["eslint:recommended", "prettier", "react-app", "react-app/jest"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "off",
    "no-undef": "off",
    "react-hooks/error-boundaries": "off",
    "react/no-unescaped-entities": "off",
    "react-hooks/immutability": "off"
  }
}